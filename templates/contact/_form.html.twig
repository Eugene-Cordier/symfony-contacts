

    {% block body %}
        {{ form_start(form) }}
        {{ form_row(form.firstname,  {'label': 'firstname'}) }}
        {{ form_row(form.lastname,  {'label': 'lastname'}) }}
        {{ form_row(form.email, {'label': 'email'}) }}
        {{ form_row(form.phone,  {'label': 'phone'}) }}
        {{ form_row(form.category,  {'label': 'category', 'placeholder': 'aucune cat√©gorie'}) }}
        {{ form_end(form, {render_rest: false}) }}
        {% if submit_label!= 'supprimer' %}
            <button class="btn btn-primary" type="submit"> {{ submit_label }} </button>
        {% endif %}
    {% endblock %}


